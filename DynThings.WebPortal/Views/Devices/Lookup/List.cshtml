@model IPagedList<DynThings.Data.Models.Device>
<table class="table striped hovered border bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ID)
                </td>
                <td>
                    <a href="#" onclick="SelectDeviceFromLookUp('@item.ID')">@item.Title</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<div id="DevicesLookupListPager">
    <input id="DeviceCurrentPage" value="@Model.PageNumber.ToString()" hidden />
    @Html.PagedListPager(Model, page => Url.Action("LookupListPV", new { page }))
</div>

