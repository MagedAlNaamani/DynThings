@using DynThings.Data.Configurations;
@model IPagedList<DynThings.Data.Models.LocationView>

@{
    ViewBag.Title = "Location Views";
}

<div class="container">
    <div class="page-header">
        <h1>Location Views</h1>
    </div>

    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogLocationViewAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtLocationViewSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_LocationViewListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_LocationViewListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>

    <br />

    <div class="well well-lg">
        <div id="divLocationViewsList">
        </div>
    </div>
</div>

<script src="~/DynScripts/LocationViews.js"></script>
<script>
    $(document).ready(function () {
        LoadPart_LocationViewListDiv();
        AttachEventDevicesListPager();
    });

</script>