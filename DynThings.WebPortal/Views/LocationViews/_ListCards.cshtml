@model IPagedList<DynThings.Data.Models.LocationView>

@foreach (var item in Model)
{
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="..." alt="...">
            <div class="caption">
                <h3>@item.Title</h3>
                <p>View type:    @item.LocationViewType.Title</p>
                <p>
                    <a href="Monitor?id=@item.ID" class="btn btn-primary" role="button">Go to Monitor</a>
                    <a href="#" class="btn btn-default" role="button">Customize</a>
                </p>
            </div>
        </div>
    </div>
}