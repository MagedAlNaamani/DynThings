@model DynThings.WebPortal.Models.ViewModels.MonitorEndPoint

<div class="accordion" data-role="accordion" data-close-any="true">

    <div class="frame active">
        <div class="heading">I\O</div>
        <div class="content" style="display: block;">
            <table class="table striped hovered bordered" id="main_table_demo">
                <thead>
                    <tr>
                        <th>Value</th>
                        <th>I/O</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var loc in Model.endPointIOs)
                    {
                        <Text>
                        <tr>
                            <td>@loc.Valu</td>
                            <td>@loc.IO</td>
                            <td>@loc.TimeStamp</td>
                        </tr>

                        </text>
                    }
                </tbody>
            </table>
        </div>
    </div>


</div>

