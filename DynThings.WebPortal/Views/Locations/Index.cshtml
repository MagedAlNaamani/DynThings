@model IEnumerable<DynThings.Data.Models.Location>
@{
    ViewBag.Title = "Locations";
}
<div class="container">
    <div class="page-header">
        <h1>Locations</h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogLocationAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtLocationsSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_LocationListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_LocationListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divLocationsList">
        </div>
    </div>
</div>

@section scripts{
<script src="~/DynScripts/Locations.js"></script>
    <script>
        $(document).ready(function () {
            LoadPart_LocationListDiv();
            
        });
    </script>
}