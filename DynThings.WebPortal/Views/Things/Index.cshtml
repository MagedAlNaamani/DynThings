@model IEnumerable<DynThings.Data.Models.Thing>
@{
    ViewBag.Title = "Things";
}
<div class="container">
    <div class="page-header">
        <h1>Things</h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <button type="button" class="btn btn-info" onclick="LoadPart_DialogThingAdd()" data-toggle="modal" data-target="#mdl">Add</button>
        </div>
        <div class="col-md-6">
            <div class="input-group">
                <input type="search" id="txtThingSearch" class="form-control" onkeydown="if (event.keyCode == 13) { LoadPart_ThingListDiv(); return false; }" />
                <div class="input-group-btn">
                    <button type="button" class="btn btn-primary" onclick="LoadPart_ThingListDiv()">Search</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="well well-lg">
        <div id="divThingsList">
        </div>
    </div>
</div>
@section scriptfiles{
    <script src="~/Assets/Scripts/DynScripts/Things.js"></script>
}
@section scripts{
    <script>
        $(document).ready(function () {
            LoadPart_ThingListDiv();
            AttachEventThingsListPager();
        });

       
    </script>
}